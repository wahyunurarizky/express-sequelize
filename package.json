{
  "name": "express-sequelize",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "npx tsc",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript, Node\" -c \"yello.bold, cyan.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "watch-ts": "tsc -w",
    "watch-node": "nodemon dist/server.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.2",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "pg": "^8.8.0",
    "sequelize": "^6.23.0",
    "sqlite3": "^5.1.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.7.18",
    "concurrently": "^7.4.0",
    "sequelize-cli": "^6.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
}
